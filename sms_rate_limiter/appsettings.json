{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimitConfig": {
    "MaxMessagesPerNumberPerSecond": 10,
    "MaxMessagesPerAccountPerSecond": 100,
    "InactivityThresholdMinutes": 30,
    "CleanupIntervalMinutes": 5
  }
}